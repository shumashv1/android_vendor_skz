<property name="Phone UI" active="true" image="/system/etc/paranoid/preferences/images/phone.jpg">

        <description><![CDATA[Hybrid mode <b><font color='green'>on</font></b><br\>Phone workspace]]></description>
        <setting type="Hybrid" target="%hybrid_mode" value="1"/>
        <setting type="Hybrid" target="com.android.systemui.dpi" value="%rom_default_dpi"/>
        <setting type="Hybrid" target="com.android.systemui.layout" value="360"/>
        <setting type="Android" target="lockscreen_clock_align_center" value="1"/>
        <setting type="Android" target="status_bar_battery" value="4"/>
        <setting type="Android" target="statusbar_toggles_enable" value="1"/>
        <setting type="Android" target="statusbar_toggles" value="2"/>
        <setting type="Android" target="statusbar_toggles_show_brightness" value="1"/>

</property>
